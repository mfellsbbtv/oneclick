      "id": "webhook-trigger",
      "name": "Webhook - Receive Provision Request",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 2,
      "position": [250, 300],
      "webhookId": "provision-main"
    },
    {
      "parameters": {
        "jsCode": "// Validate incoming request data\nconst data = $input.first().json;\nconst errors = [];\n\n// Check required fields\nif (!data.employee?.fullName) {\n  errors.push('Employee full name is required');\n}\nif (!data.employee?.workEmail) {\n  errors.push('Employee work email is required');\n}\n\n// Check at least one app is enabled\nconst apps = data.applications || {};\nconst enabledApps = Object.keys(apps).filter(app => apps[app]?.enabled);\nif (enabledApps.length === 0) {\n  errors.push('At least one application must be enabled');\n}\n\n// Generate request ID if not provided\nconst requestId = data.requestId || `prov-${Date.now()}-${Math.random().toString(36).substr(2, 9)}`;\n\nif (errors.length > 0) {\n  return {\n    valid: false,\n    requestId,\n    errors,\n    status: 'validation_failed'\n  };\n}\n\n// Return validated data with request ID\nreturn {\n  valid: true,\n  requestId,\n  employee: data.employee,\n  applications: apps,\n  enabledApps,\n  timestamp: new Date().toISOString(),\n  status: 'validated'\n};"
      },
      "id": "validate-request",
      "name": "Validate Request Data",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [450, 300]
    },
    {
      "parameters": {
        "rules": {
          "values": [
            {
              "conditions": {
                "conditions": [
                  {
                    "leftValue": "={{ $json.valid }}",
                    "rightValue": true,
                    "operation": "equal"
                  }
                ]
              },
              "renameOutput": true,
              "outputKey": "Valid"
            },
            {
              "conditions": {
                "conditions": [
                  {
                    "leftValue": "={{ $json.valid }}",
                    "rightValue": false,
                    "operation": "equal"
                  }
                ]
              },
              "renameOutput": true,
              "outputKey": "Invalid"
            }
          ]
        }
      },
