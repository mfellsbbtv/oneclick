        },
        "workflowData": "={{ JSON.stringify({\n  requestId: $('init-status').item.json.requestId,\n  employee: $('init-status').item.json.employee,\n  config: $('init-status').item.json.applications.microsoft365\n}) }}"
      },
      "id": "execute-microsoft",
      "name": "Execute Microsoft 365 Workflow",
      "type": "n8n-nodes-base.executeWorkflow",
      "typeVersion": 1,
      "position": [1250, 540]
    },
    {
      "parameters": {
        "jsCode": "// Aggregate results from all sub-workflows\nconst results = {};\nconst allInputs = $input.all();\n\n// Process results from each app workflow\nallInputs.forEach(input => {\n  if (input.json.application) {\n    results[input.json.application] = {\n      status: input.json.status,\n      userId: input.json.userId,\n      details: input.json.details,\n      error: input.json.error,\n      completedAt: input.json.completedAt\n    };\n  }\n});\n\n// Calculate overall status\nconst statuses = Object.values(results).map(r => r.status);\nlet overallStatus = 'completed';\nif (statuses.includes('failed')) {\n  overallStatus = statuses.every(s => s === 'failed') ? 'failed' : 'partial';\n}\n\nreturn {\n  requestId: $('init-status').item.json.requestId,\n  status: overallStatus,\n  timestamp: new Date().toISOString(),\n  employee: $('init-status').item.json.employee,\n  results,\n  summary: {\n    total: Object.keys(results).length,\n    succeeded: statuses.filter(s => s === 'success').length,\n    failed: statuses.filter(s => s === 'failed').length\n  }\n};"
      },
      "id": "aggregate-results",
      "name": "Aggregate Results",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [1450, 400]
    },
